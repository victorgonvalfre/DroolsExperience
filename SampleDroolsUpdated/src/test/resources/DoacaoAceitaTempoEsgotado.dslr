package br.edu.sr.ifes.mpn.model;
import br.edu.sr.ifes.mpn.model.*;

expander DoacaoAceitaTempoEsgotado.dsl
rule "DoacaoAceitaMasOTempoEstourou "
	when 
	 	Validar notificacao
	 	Verificar se Doacao foi aceita 
	 	Checkar timer esgotado 21600000
	then
		Display msgDoacaoTempoEsgotado
		Deleta Doacao
		Deleta Notificacao
end